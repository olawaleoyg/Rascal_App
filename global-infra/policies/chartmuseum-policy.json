

{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowListObjects",
        "Effect": "Allow",
        "Action": [
          "s3:ListBucket"
        ],
        "Resource": "arn:aws:s3:::samanja-io-chartmuseum"
      },
      {
        "Sid": "AllowObjectsCRUD",
        "Effect": "Allow",
        "Action": [
          "s3:DeleteObject",
          "s3:GetObject",
          "s3:PutObject"
        ],
        "Resource": "arn:aws:s3:::samanja-io-chartmuseum/*"
      }
    ]
}